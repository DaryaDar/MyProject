<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#8BC34A</item>
        <item name="colorPrimaryDark">#4CAF50</item>

        <item name="android:windowBackground">#CDDC39</item>
        <item name="android:itemBackground">#39DCAE</item>
        <item name="actionBarTheme">@style/MyActionBar2</item>
        <item name="android:buttonStyle">@style/ButtonStyle2</item>
        <item name="imageButtonStyle">@style/ButtonStyle2</item>
        <item name="android:textViewStyle">@style/TextView2</item>
    </style>

    <style name="MyActionBar2" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorPrimary">#000000</item>
    </style>

    <style name="ButtonStyle2" parent="Base.Widget.AppCompat.Button">
        <item name="android:textColor">#000000</item>
        <item name="android:background">@color/new_selector</item>
    </style>

    <style name="TextView2" parent="Base.Widget.AppCompat.TextView">
        <item name="android:textColor">#000000</item>
    </style>
</resources>